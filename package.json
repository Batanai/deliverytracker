    "babel-jest": "^26.6.3",
    "concurrently": "^7.6.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "postcss": "^8.4.21",
    "react-test-renderer": "18.1.0",
    "tailwindcss": "^3.2.6",
